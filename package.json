{
  "name": "webpack-karma-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:tests": "karma start karma.watch.config.js",
    "tests": "karma start karma.single.config.js",
    "test": "npm run tests",
    "dev": "parallelshell \"webpack --config webpack.dev.config.js --progress --colors\"",
    "watch": "parallelshell \"npm run server\" \"webpack --config webpack.dev.config.js --progress --colors --watch\" ",
    "build": "set NODE_ENV=production npm run tests && webpack --config webpack.prod.config.js --progress --colors",
    "server": "live-server build/ --port=3000",
    "start": "npm run server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^2.2.0",
    "css-loader": "^0.14.2",
    "file": "^0.2.2",
    "jsx-loader": "^0.13.1",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.8",
    "karma-cli": "0.0.4",
    "karma-mocha": "^0.1.10",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.5.0",
    "live-server": "^0.7.1",
    "mocha": "^2.2.4",
    "node-libs-browser": "^0.5.2",
    "parallelshell": "^1.1.1",
    "rewire": "^2.3.3",
    "rewire-webpack": "^1.0.0",
    "sinon": "git://github.com/cjohansen/Sinon.JS#sinon-2.0",
    "style-loader": "^0.12.3",
